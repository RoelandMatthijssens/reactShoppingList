{
    "name": "ShoppingList",
    "description": "REST backend for shoppinglist",
    "version": "0.0.1",
    "main": "index.js",
    "scripts": {
        "start": "node src/back/main.js",
        "dev:back": "ENV=development nodemon --watch ./src/back/ src/back/main.js",
        "dev:front": "ENV=development webpack-dev-server --config=./src/config/webpack.config.js",
        "build": "webpack --config=./src/config/webpack.config.js && cp -r src/front/static/* ./build/dist/."
    },
    "dependencies": {
        "body-parser": "*",
        "eslint": "^4.18.2",
        "express": "^4.14.0",
        "normalize.css": "^8.0.0",
        "react": "^16.2.0",
        "react-dom": "^16.2.0",
        "react-redux": "^5.0.7",
        "redux": "^3.7.2",
        "redux-promise": "^0.5.3",
        "redux-thunk": "^2.2.0",
        "sequelize": "^4.13.4",
        "sqlite3": "^3.1.13"
    },
    "devDependencies": {
        "babel-core": "^6.26.0",
        "babel-loader": "^7.1.4",
        "babel-preset-env": "^1.6.1",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-3": "^6.24.1",
        "css-loader": "^0.28.10",
        "extract-text-webpack-plugin": "^4.0.0-beta.0",
        "file-loader": "^1.1.11",
        "nodemon": "^1.17.2",
